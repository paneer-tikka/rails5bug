<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>Gender</th>
      <th>Dob</th>
      <th>Password</th>
      <th>Country</th>
      <th>State</th>
      <th>City</th>
      <th>Location</th>
      <th>Address</th>
      <th>Pin code</th>
      <th>Status</th>
      <th>Profile picture</th>
      <th>Owner</th>
      <th>Owner type</th>
      <th>Api key</th>
      <th>Secret</th>
      <th>Creator</th>
      <th>Updater</th>
      <th>Deleter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.title %></td>
        <td><%= user.first_name %></td>
        <td><%= user.middle_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.mobile %></td>
        <td><%= user.gender %></td>
        <td><%= user.dob %></td>
        <td><%= user.password %></td>
        <td><%= user.country_id %></td>
        <td><%= user.state_id %></td>
        <td><%= user.city_id %></td>
        <td><%= user.location_id %></td>
        <td><%= user.address %></td>
        <td><%= user.pin_code %></td>
        <td><%= user.status %></td>
        <td><%= user.profile_picture %></td>
        <td><%= user.owner_id %></td>
        <td><%= user.owner_type %></td>
        <td><%= user.api_key %></td>
        <td><%= user.secret %></td>
        <td><%= user.creator_id %></td>
        <td><%= user.updater_id %></td>
        <td><%= user.deleter_id %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
